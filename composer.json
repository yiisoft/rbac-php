{
    "name": "yiisoft/rbac-php",
    "type": "library",
    "description": "Yii RBAC PHP file storage",
    "keywords": [
        "yii",
        "rbac",
        "storage"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/rbac",
        "issues": "https://github.com/yiisoft/rbac/issues",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yiisoft/rbac": "^3.0@dev",
        "yiisoft/var-dumper": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "psr/log": "^1.1.3",
        "roave/infection-static-analysis-plugin": "^1.14",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.2",
        "yiisoft/files": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Rbac\\Php\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Rbac\\Php\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    }
}
